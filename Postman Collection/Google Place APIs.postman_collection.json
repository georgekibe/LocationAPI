							"pm.test(\"Response status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							"pm.test(\"Place_id and key parameters are present in the request URL\", function () {\r",
							"    const requestUrl = pm.request.url.toString();\r",
							"    \r",
							"    pm.expect(requestUrl).to.include(\"place_id\").and.to.include(\"key\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response includes the expected message for successful update\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"\r",
							"    pm.expect(responseData.msg).to.equal(\"Address successfully updated\");\r",
							"});\r",
							"\r",
							""
						],
